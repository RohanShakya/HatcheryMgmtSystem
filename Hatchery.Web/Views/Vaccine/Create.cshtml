@model Nepaltech.Models.Forms.VaccineModel

@{
    ViewBag.Title = "Create";
    Layout = null;
}
<form action="@(Url.Action("Create"))" method="post" class="nt-form" id="form-add-vaccine">
    @Html.AntiForgeryToken()

 
      
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="row">
            <div class="col-md-3"> @Html.LabelFor(model => model.VaccineName) </div>
            <div class="col-md-6">
                @Html.Kendo().TextBoxFor(m => m.VaccineName)
                @Html.ValidationMessageFor(model => model.VaccineName, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-0 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </form>

